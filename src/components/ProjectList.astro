    <div class="flex flex-wrap justify-center">
      <Project
        title="Reinforcement Learning V2"
        description="Rebuilt my reinforcement learning project to be up to date"
        href="https://github.com/wanderdust/reinforcement-learning/tree/main"
        image="/images/frozen-lake.png"
        tags={["Reinforcement Learning", "Classical AI", "Q-Networks"]}
      />

      <Project
        title="Enhancing Alana for Eldery care"
        description="A voice assistant designed to assist eldery people"
